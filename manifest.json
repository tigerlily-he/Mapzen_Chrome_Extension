{
  "manifest_version": 2,

  "name": "Mapzen",
  "description": "Locates an address using Mapzen's Tangram and Search",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "unlimitedStorage",
    "geolocation",
    "contextMenus",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "commands": {
          "_execute_browser_action": {
            "suggested_key": {
              "windows": "Ctrl+9",
              "mac": "MacCtrl+9"
            }
          }
        }
}
